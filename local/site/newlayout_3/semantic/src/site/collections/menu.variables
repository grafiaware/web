/*******************************
    User Variable Overrides
*******************************/

/*Menu*/
@border: 0;
@boxShadow: 0 0;

/*Text Menu*/
@textMenuItemSpacing: @relative2px; 
@textMenuItemPadding: @relative3px @textMenuItemSpacing; 
@textLineHeight: 1;

/*Sizes*/
@mediumWidth: 100%;

/*Sub Menu*/
@subMenuFontSize: @relativeMedium;

/* Button */
@buttonSize: @relativeSmall;

/* Resize large sizes */
@tiny: @11px;
